/*ͷ��header*/
.index-nav {
  padding: 6px 0;
}
.index-nav .index-inner {
  text-align: center;
  margin-top: 16px;
}
.index-nav .index-inner > a {
  display: block;
}
.index-nav .index-inner > a > img {
  width: 50px;
  margin: 0 auto;
}
.index-nav .index-inner > a > p {
  color: #5a5a5a;
  margin-top: 3px;
  font-size: 13px;
}
.index-nav .index-inner:nth-last-child(-n+4) {
  display: none;
}
.index-title-head {
  height: 35px;
  background-color: #ff9000;
  background: -webkit-gradient(linear, 0 0, 0 100%, from(#ffaf47), to(#ff9000));
  border-bottom: solid 1px #ffc881;
  line-height: 35px;
  overflow: hidden;
  color: #fff;
  padding-left: 10px;
  font-weight: bold;
  font-size: 18px;
  clear: both;
}
.index-title-head > span {
  float: left;
}
.index-title-head > img {
  width: 20px;
  float: right;
  margin-right: 10px;
  color: #FFF;
  margin-top: 8px;
}
.index-cuxiao > a {
  display: block;
  width: 100%;
}
.index-cuxiao > .index-cuxiao-pro {
  padding-bottom: 10px;
  float: left;
  width: 100%;
}
.index-cuxiao > .index-cuxiao-pro > .index-cuxiaoPro-list {
  background-color: #ffffff;
  text-align: left;
  font-size: 14px;
  max-width: 960px;
  margin: 0px auto;
}
.index-cuxiao > .index-cuxiao-pro > .index-cuxiaoPro-list li {
  padding: 14px 8px;
  position: relative;
}
.index-cuxiao > .index-cuxiao-pro > .index-cuxiaoPro-list li > a {
  display: block;
}
.index-cuxiao > .index-cuxiao-pro > .index-cuxiaoPro-list li > a > .pic {
  float: left;
  width: 110px;
}
.index-cuxiao > .index-cuxiao-pro > .index-cuxiaoPro-list li > a > .pic > img {
  width: 100px;
}
.index-cuxiao > .index-cuxiao-pro > .index-cuxiaoPro-list li > a > .info {
  padding-left: 90px;
  font-size: 14px;
  line-height: 24px;
}
.index-cuxiao > .index-cuxiao-pro > .index-cuxiaoPro-list li > a > .info > .title {
  color: #333333;
  word-break: break-all;
  font-size: 15px;
  line-height: 19px;
  min-height: 57px;
  text-align: left;
  font-weight: normal;
}
.index-cuxiao > .index-cuxiao-pro > .index-cuxiaoPro-list li > a > .info > .title > span {
  color: #ff6c00;
  display: inline;
}
.index-cuxiao > .index-cuxiao-pro > .index-cuxiaoPro-list li > a > .info > .other {
  margin-top: 8px;
  color: #a1a1a1;
}
.index-cuxiao > .index-cuxiao-pro > .index-cuxiaoPro-list li > a > .info > .other > .mall {
  color: #a1a1a1;
  font-size: 12px;
  float: left;
}
.index-cuxiao > .index-cuxiao-pro > .index-cuxiaoPro-list li > a > .info > .other > .common {
  color: #a1a1a1;
  font-size: 12px;
  float: right;
}
.index-cuxiao > .index-cuxiao-pro > .index-cuxiaoPro-list li > a > .info > .other > .common > i {
  background-image: url("data:image/svg+xml;charset=utf-8,<svg xmlns='http://www.w3.org/2000/svg' viewBox='0 0 1024 1024' width='17'><path fill='#a1a1a1' d='M924.504962 664.898711 97.819886 664.898711c-17.557888 0-31.795147-14.241352-31.795147-31.795147l0-508.732583c0-17.557888 14.238282-31.792077 31.795147-31.792077l241.198387 0 149.664671-149.661601c6.209422-6.212492 14.34266-9.31618 22.478967-9.31618s16.269545 3.102664 22.479991 9.31618L683.306574 92.57788l241.198387 0c17.557888 0 31.795147 14.235213 31.795147 31.792077L956.300109 633.10254C956.300109 650.657358 942.061826 664.898711 924.504962 664.898711zM892.708791 156.169197l-222.569098 0c-8.428973 0-16.518209-3.354398-22.478967-9.31618l-136.498814-136.494721-136.498814 136.494721c-5.961782 5.961782-14.051017 9.31618-22.479991 9.31618l-222.569098 0L129.61401 601.304324l763.093758 0L892.707768 156.169197zM288.592814 378.735226m-63.591317 0a62.143 62.143 0 1 0 127.182634 0 62.143 62.143 0 1 0-127.182634 0ZM511.161912 378.735226m-63.591317 0a62.143 62.143 0 1 0 127.182634 0 62.143 62.143 0 1 0-127.182634 0ZM733.73101 378.735226m-63.591317 0a62.143 62.143 0 1 0 127.182634 0 62.143 62.143 0 1 0-127.182634 0Z' transform='translate(0, 812) scale(1, -1)'/></svg>");
  width: 14px;
  height: 15px;
  display: inline-block;
  background-size: auto 100%;
  background-repeat: no-repeat;
  background-position: center;
  vertical-align: middle;
  margin-right: 4px;
}
.index-cuxiao > .index-cuxiaoPro-morebar {
  width: 100%;
  text-align: center;
}
.index-cuxiao > .index-cuxiaoPro-morebar > a {
  display: block;
  height: 30px;
  line-height: 30px;
  background-color: #f4f4f4;
  font-size: 13px;
  background-image: linear-gradient(#ffffff, #f4f4f4);
  margin: 10px;
  border: 1px solid #dddddd;
  border-radius: 4px;
}
.index-top10-bar > a {
  display: block;
  width: 100%;
}
footer > .index-foot-top {
  margin-top: 10px;
  background: #efefef;
  border-top: 1px solid #c4c4c4;
  border-bottom: 1px solid #c4c4c4;
  height: 34px;
  line-height: 34px;
  clear: both;
}
footer > .index-foot-top > a {
  float: left;
  font-size: 14px;
  border-right: 1px solid #c4c4c4;
  text-align: center;
  width: 30%;
}
footer > .index-foot-top > a:last-child {
  width: 40%;
  border-right: none;
}
footer > .index-foot-top > a:last-child > img {
  width: 15px;
  display: inline-block;
  margin-top: -5px;
  margin-right: 2px;
}
footer > .index-foot-bottom {
  background: #efefef;
  text-align: center;
  font-size: 12px;
  padding: 15px 0;
  color: #666;
}
footer > .index-foot-bottom > p:first-child > a:first-child {
  color: red;
}
